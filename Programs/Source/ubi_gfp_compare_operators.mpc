
a = sint.get_input_from(0)
b = sint.get_input_from(1)

a_gt_b = a > b
clr_a_gt_b = a_gt_b.reveal()
print_ln('a-gt-b=%s', clr_a_gt_b)

a_st_b = a < b
clr_a_st_b = a_st_b.reveal()
print_ln('a-st-b=%s', clr_a_st_b)

a_eq_b = a == b
clr_a_eq_b = a_eq_b.reveal()
print_ln('a-eq-b=%s', clr_a_eq_b)

a_ne_b = a != b
clr_a_ne_b = a_ne_b.reveal()
print_ln('a-ne-b=%s', clr_a_ne_b)

c_a = a.reveal()
c_b = b.reveal()

print_ln('a=%s;', c_a)
print_ln('b=%s;', c_b)
